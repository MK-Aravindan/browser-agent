OPENAI_API_KEY=
GOOGLE_API_KEY=

PROVIDER=auto
OPENAI_MODEL=gpt-5-mini
GEMINI_MODEL=gemini-2.5-flash
# MODEL=

CDP_PORT=9222
CONNECT_EXISTING_CDP=true
BROWSER_MODE=auto
# auto|own|fresh|managed
# CHROME_EXECUTABLE_PATH=C:\Program Files\Google\Chrome\Application\chrome.exe
FRESH_CHROME_USER_DATA_DIR=.browser-agent/chrome-fresh-profile
FRESH_CHROME_START_TIMEOUT=45
PROFILE_DIRECTORY=Default
# BROWSER_CHANNEL=chrome
ENABLE_DEFAULT_EXTENSIONS=false

HEADLESS=false
KEEP_ALIVE=false

MAX_STEPS=60
MAX_ACTIONS_PER_STEP=6
FLASH_MODE=true
USE_THINKING=true
USE_VISION=true

MIN_PAGE_LOAD_WAIT=0.25
NETWORK_IDLE_WAIT=1.0
WAIT_BETWEEN_ACTIONS=0.15
HIGHLIGHT_ELEMENTS=false

# Comma-separated lists
# ALLOWED_DOMAINS=example.com,docs.python.org
INCLUDE_ATTRIBUTES=id,name,role,type,value,placeholder,aria-label,data-testid,href,title,alt

LOG_LEVEL=INFO
LOG_FILE=logs/browser_agent.log
LOG_STEP_DETAILS=true
LOG_DOM_TAG_SUMMARY=false
